using Common;
using TPM3.Sys;

namespace TPM3.wx
{
    /// <summary>
    /// 软件更动说明，包括4个部分
    /// </summary>
    [TypeNameMap("wx.RegressModifyInfo")]
    public partial class RegressModifyInfo : MyBaseForm
    {
        public RegressModifyInfo()
        {
            InitializeComponent();
            flex1.modifyType = "适应性改造更动";
            flex2.modifyType = "新增功能";
            flex3.modifyType = "其他更动";
        }

        public override bool OnPageCreate()
        {
            flex1.OnPageCreate();
            flex2.OnPageCreate();
            flex3.OnPageCreate();
            flex4.OnPageCreate();
            return true;
        }

        public override bool OnPageClose(bool bClose)
        {
            flex1.OnPageClose();
            flex2.OnPageClose();
            flex3.OnPageClose();
            flex4.OnPageClose();
            return true;
        }
    }
}